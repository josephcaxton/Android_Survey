<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" android:id="@+id/LLMultipleSingle">
    
   <WebView android:id="@+id/QuestionHeaderBox" android:layout_width="match_parent" android:layout_height="160dp" 
    android:gravity="center"  android:layout_alignParentTop="true">
    </WebView>
    
        <ScrollView android:id="@+id/scrollMultipleSinglePg" 
     android:layout_width="wrap_content"
     android:layout_height="wrap_content" android:layout_below="@+id/QuestionHeaderBox">
     
     <RelativeLayout
            android:id="@+id/relativelayoutMultipleSingle2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"  >
    
    <TableLayout android:id="@+id/Qtable"  android:layout_width="fill_parent" 
    android:layout_height="wrap_content" android:gravity="center"  
   >
        
    </TableLayout>
    
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"  
    android:id="@+id/btnSingleAnswerNext" android:text="@string/Next" android:layout_marginRight="10dp" 
     android:layout_alignParentBottom="true" android:layout_alignParentRight="true" android:layout_below="@+id/Qtable">
    </Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"  
    android:id="@+id/btnSingleStop" android:text="@string/Stop" android:layout_marginLeft="10dp" 
      android:layout_alignParentBottom="true" android:layout_alignParentLeft="true" android:layout_below="@+id/Qtable">
    </Button>
    
    </RelativeLayout>
		
	 </ScrollView>
</RelativeLayout>

